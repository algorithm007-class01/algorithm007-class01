#include <iostream>
#include <cassert>
#include <vector>

using namespace std;

// 暴力解法
// 时间复杂度: O(n^3)
// 空间复杂度: O(1)
class Solution {
public:
    int minSubArrayLen(int s, vector<int>& nums) {

        int res = nums.size() + 1;
        for( int l = 0 ; l < nums.size() ; l ++ )
            for( int r = l ; r < nums.size() ; r ++ ){
                int sum = 0;
                for( int i = l ; i <= r ; i ++ )
                    sum += nums[i];
                if( sum >= s )
                    res = min( res , r - l + 1 );
            }

        if( res == nums.size() + 1 )
            return 0;

        return res;
    }
};

